<resources>
	<string id="AppName">CdAMeter</string>
	<string id="fieldTitle">Calculated CdA</string>
	<string id="fieldLabel">CdA</string>
	<string id="fieldUnit">m^2</string>
	<string id="settingCRR">CRR</string>
	<string id="settingMass">mass</string>

	<bitmap id="LauncherIcon" filename="test.png" />

	<fitContributions>
		<fitField 
			id="0" 
			displayInChart="true" 
			sortOrder = "0" 
			precision="2"
			chartTitle="@Strings.fieldTitle"
			dataLabel="@Strings.fieldLabel"
			unitLabel="@Strings.fieldUnit"
			fillColor="#FF0000"
		/>
		<fitField 
			id="1" 
			displayInChart="false" 
			sortOrder = "0" 
			precision="2"
			chartTitle="@Strings.fieldTitle"
			dataLabel="@Strings.fieldLabel"
			unitLabel="@Strings.fieldUnit"
			fillColor="#FF0000"
		/>
	</fitContributions>

	<settings>
		<setting propertyKey="@Properties.crr" title="@Strings.settingCRR">
			<settingConfig
				type="numeric"
				min="0"
				max="1"
			/>
		</setting>

		<setting propertyKey="@Properties.mass" title="@Strings.settingMass">
			<settingConfig
				type="numeric"
				min="0"
				max="200"
			/>
		</setting>
	</settings>
</resources>

